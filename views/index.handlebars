<div id="home">

  <div class="container">

    <div class="row">

      <div class="col-8 offset-2">

        <h1>{{msg}}</h1>
        

        {{#if loginPage}}
        <h1>not </h1>
        {{/if}}
        <h1>already a member</h1>
        {{#if loginPage}}
        <h1 id="signupBtn" data-status="false">SIGN UP</h1>
        {{else}}
        <h1 id="loginBtn" data-status="true">SIGN IN</h1>
        {{/if}}
        
        {{#if loginPage}}
        <h1>login page</h1>
        {{> login/login}}
        {{/if}}
        {{#if signupPage}}
        <h1>signup page</h1>
        {{> signup/signup}}
        {{/if}}
        {{#if giftPage}}
        <h1>gifts page</h1>
        {{> gifts/gifts}}
        {{/if}}

        

      </div>

    </div>

  </div>
  
</div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<script src="/js/js.js"></script>
