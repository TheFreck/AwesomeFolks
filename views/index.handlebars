<div id="home">

  <div class="container">

    <div class="row">

      <div class="col-8 offset-2">

        <h1>{{msg}}</h1>
        
        <p><span id="isMember" class="show">not</span> already a member? <span id="loginBtn">SIGN UP</span></p>
        
        {{#if loginPage}}
        <h1>login page</h1>
        {{> login/login}}
        {{/if}}
        {{#if signupPage}}
        <h1>signup page</h1>
        {{> signup/signup}}
        {{/if}}
        {{#if giftPage}}
        <h1>gifts page</h1>
        {{> gifts/gifts}}
        {{/if}}

        

      </div>

    </div>

  </div>
  
</div>


<script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>

<script src="/js/js.js"></script>
