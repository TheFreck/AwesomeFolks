<div class="container">
  <div class="row">
    <div class="col-8 offset-2">
        <h2>The Greedy Bastard List</h2>
        {{>navigation/nav-users}}
        {{>navigation/nav-logout}}
        <h3>BUY ME THIS....NOW!</h3>
    </div>
  </div>
  <div class="row">
    <div class="col-8 offset-2">
      <ul id="wishList" class="list-group">
        {{#each gift}}
          <li data-id="{{this.id}}" data-uuid="{{this.userUuid}}" class="list-group-item">
            <h4>{{this.item}}</h4>
            <p>{{this.category}}</p>
            <p>{{this.price}}</p>
            <p>{{this.comments}}</p>
            <button class="btn btn-success float-right delete">PURCHASE</button>
            <form action="http://www.google.com/search" method="get">
              {{!-- <input type="text" class="itemInput" name="q" value="{{this.item}}"/> --}}
              <button type="submit" target="_blank" value="Find Me Online">Find Me Online</button>
            </form>
          </li>
        {{/each}}
      </ul>
    </div>
  </div>
</div>









